dbody {
	text-align: center;
	color: white;
	background-image: url("/assets/img/fire.gif");
	background-repeat: repeat;
	background-size: 5% 25%;
	
	text-shadow: red 1px 0 10px;
}

img {
	box-shadow: black 1px 0 10px;
}


 /* unvisited link */
a:link {
  color: white;
}

/* visited link */
a:visited {
  color: white;
}

/* mouse over link */
a:hover {
  color: white;
}

/* selected link */
a:active {
  color: white;
} 

.uploaded{
	height: 150px;
	width: 150px;
	margin: 3px;
	object-fit: cover;
}
